{
	"name": "codec-demo",
	"targetType":"executable",
    "description": "Custom codec demo for hunt-net.",
    "copyright": "Copyright (C) 2018-2020, HuntLabs",
    "homepage": "https://www.huntlabs.net",
    "license": "Apache-2.0",
    "versions": ["HUNT_DEBUG", "HUNT_IO_DEBUG", "HUNT_NET_DEBUG"],
	"dependencies":{
		"hunt-net":{
			"path" :"../../"
		}
	},
	"subConfigurations": {
		"hunt-net" : "ssl"
	},
    "configurations": [
		{
			"name": "client",
			"targetName": "codec-client",
			"targetType": "executable",
			"excludedSourceFiles": [
				"source/server.d"
			]
		},
		{
			"name": "server",
			"targetName": "codec-server",
			"targetType": "executable",
			"excludedSourceFiles": [
				"source/client.d"
			]
		}
	]
}